{
  "name": "react-modal-back-button",
  "version": "1.0.6",
  "description": "A lightweight React hook that enables native back button support for modals and dialogs. Press the back button to close modals instead of navigating away.",
  "keywords": [
    "react",
    "react-hook",
    "modal",
    "dialog",
    "back-button",
    "back button",
    "browser-back",
    "history",
    "history-api",
    "popstate",
    "navigation",
    "mobile",
    "mobile-ux",
    "android",
    "ios",
    "pwa",
    "webview",
    "typescript",
    "modal-dialog",
    "close-modal",
    "dismiss",
    "overlay",
    "popup",
    "ux",
    "user-experience"
  ],
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "prepublishOnly": "npm run build"
  }
}
